create table agree_dev (galcount int primary key, r_bulge_tot float default -999,r_bulge_gr float default -999,r_bulge_gi float default -999,r_bulge_ri float default -999,n_bulge_tot float default -999,n_bulge_gr float default -999,n_bulge_gi float default -999,n_bulge_ri float default -999,ba_bulge_tot float default -999,ba_bulge_gr float default -999,ba_bulge_gi float default -999,ba_bulge_ri float default -999,pa_bulge_tot float default -999,pa_bulge_gr float default -999,pa_bulge_gi float default -999,pa_bulge_ri float default -999,r_disk_tot float default -999,r_disk_gr float default -999,r_disk_gi float default -999,r_disk_ri float default -999,ba_disk_tot float default -999,ba_disk_gr float default -999,ba_disk_gi float default -999,ba_disk_ri float default -999,pa_disk_tot float default -999,pa_disk_gr float default -999,pa_disk_gi float default -999,pa_disk_ri float default -999,BT_tot float default -999,BT_gr float default -999,BT_gi float default -999,BT_ri float default -999,total float default -999,total_gr float default -999,total_gi float default -999,total_ri float default -999);
insert into agree_dev (galcount) select galcount from catalog.CAST; 

